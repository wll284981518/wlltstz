/* pages/index/index.wxss */
/* 最大盒子 */
.wll_index{
  width: 100%;
  height: 100%;
}

/* 头部 */
.wll_head{
  width: 100%;
  height: 376rpx;
  position: relative;
}

/* 头部输入框 */
.wll_search .wll_input{
  width: 604rpx; 
  height: 64rpx;
  background: #fff;
  font-size: 26rpx;
  margin: 0 auto;
  padding-left: 36rpx;
  z-index: 1;
  /* background-image:url(/tianshi_03.jpg) no-repeat; */
  outline: none;
  border-radius: 30rpx;
  position: absolute;
  top: 26rpx;
  left: 56rpx;
}

.wll_image1{
  width:48rpx;
  height:48rpx;
  position: absolute;
  margin-top: 36rpx;
  margin-left: 616rpx;
  z-index: 8;
}

/* 轮播图 */
.wll_swiper{
  width: 100%;
  height: 376rpx;
}

/* 轮播图片 */
.wll_img{
  width: 100%;
  height: 100%;
}

/* 通知栏图片 */
.wll_scroll{
  width: 100%;
  height:90rpx;
  line-height: 90rpx;
  border-bottom: 1px solid darkgray;
}

.wll_Scroll_content{
  width: 90%;
  height:90rpx;
  line-height: 90rpx;
  display: flex;
  align-items: center;
  margin: 0 auto;
}

.wll_image2{
  width: 38rpx;
  height:38rpx;
}

.wll_swiper_container{
  width: 80%;
  height: 70rpx;
  align-items: center;
  margin-top: -20rpx;
  padding-left: 16rpx;
}

.wll_swiper_item{
  font-size:  26rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #e43633;
}

.wll_text1{
  color: darkgray;
  font-size: 22rpx;
}

/* 导航列表 */
.wll_userlist{
  width: 100%;
  height: 410rpx;
}

/* ul */
.wll_userlist .wll_list_top{
  width: 700rpx;
  height: 320rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
  margin: 0 auto;
}

/* li */
.wll_view{
  width: 20%;
  height: 106rpx;
  /* margin-bottom: 52rpx;
  display: flex; */
  flex-direction: column;
}

/* li里的图片 */
.wll_userlistimg{
  width: 60rpx;
  height: 60rpx;
  margin: 24rpx auto;
  display: block;
}

/* li里的字 */
.wll_p{
  font-size: 20rpx;
  text-align: center;
  display: block;
}

/* 爆品推荐 */
.wll_products{
  width: 466rpx;
  height: 114rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.wll_products view{
  width: 164rpx;
  height: 3rpx;
  background: #e0e0e0;
}

.wll_products text{
  font-size: 24rpx;
  color: #909090;
  font-weight: bold;
}

/* 计时器 */
.wll_timer{
  height: 100%;
  display: flex;
  flex-wrap: wrap;
  background: #f1f1f1;
  padding: 10rpx;
  justify-content: space-around;
}

.wll_timer .wll_li1{
  width: 340rpx;
  height: 470rpx;
  background: #fff;
  margin-top: 36rpx;
  border-radius: 10rpx;
}

.wll_li1 image{
  width: 340rpx;
  height: 340rpx;
  border-radius: 20rpx;
  display: block;
}

.wll_name1{
  width: 300rpx;
  height: 60rpx;
  display: block;
  line-height: 70rpx;
  margin: 0 auto;
  /* margin-left: 30rpx; */
  font-size: 26rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.wll_price1{
  display: flex;
  height: 60rpx;
  justify-content: space-around;
  align-items: center;
}

.wll_minPrice{
  display: block;
  width: 30%;
  color: #e43633;
  font-size: 28rpx;
}

.wll_originalPrice{
  display: block;
  width: 30%;
  text-decoration: line-through;
  color: #9d9d9d;
  font-size: 28rpx;
}

/* api小工厂 */
.wll_apiimg{
  width: 100%;
  height: 260rpx;
  margin-top: 56rpx;
}

.wll_apiimg image{
  width: 100%;
  height: 260rpx;
}

